{
  "extends": "next/core-web-vitals",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "import/no-unresolved": "off",
    "import/named": "off",
    "no-console": "warn",
    "react-hooks/exhaustive-deps": "off",
    "prefer-const": "off",
    "no-var": "error"
  }
}
